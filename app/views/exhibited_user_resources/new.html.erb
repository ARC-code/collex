  <tbody>
    <tr>
      <%= td f.label :title, "Title" %>
      <%= td f.text_field :title %>
    </tr>
    <tr>
      <%= td f.label :url, "URL" %>
      <%= td f.text_field :url %>
    </tr>
    <tr>
      <%= td f.label :role_AUT, "Author" %>
      <%= td f.text_field :role_AUT %>
    </tr>
    <tr>
      <%= td f.label :role_EDT, "Editor" %>
