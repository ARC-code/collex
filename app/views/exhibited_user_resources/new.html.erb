    <tr>
      <%= td f.label :role_PBL, "Publisher" %>
      <%= td f.text_field :role_PBL %>
    </tr>
    <tr>
      <%= td f.label :pubPlace, "PubPlace" %>
      <%= td f.text_field :pubPlace %>
    </tr>
    <tr>
      <%= td f.label :date_label, "Date" %>
      <%= td f.text_field :date_label %>
    </tr>
    <tr>
      <%= td f.label :pages, "Pages" %>
      <%= td f.text_field :pages %>
    </tr>
    <tr>
      <%= td f.label :annotation, "annotation" %>
